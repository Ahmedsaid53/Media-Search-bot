{
    "name": "Media Search bot",
    "description": "easily share private channel files using Telegram InLine search, without database",
    "keywords": [
      "telegram",
      "best",
      "indian",
      "telethon",
      "media",
      "search",
      "channel",
      "index",
      "inline"
    ],
    "website": "https://github.com/SpEcHiDe/Media-Search-bot",
    "repository": "https://github.com/SpEcHiDe/Media-Search-bot",
    "env": {
        "TG_BOT_TOKEN": {
            "description": "Your bot token."
        },
        "APP_ID": {
            "description": "Get this value from https://my.telegram.org"
        },
        "API_HASH": {
            "description": "Get this value from https://my.telegram.org",
        },
        "TG_USER_SESSION": {
            "description": "Your String Session"
        },
        "TG_DUMP_CHAT": {
            "description": "Username or ID of channel or group."
        }
    },
    "addons": [],
    "buildpacks": [{
        "url": "heroku/python"
    }],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
